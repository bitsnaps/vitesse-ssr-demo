{
  "private": true,
  "name": "vitesse-ssr-demo",
  "type": "module",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "vite-ssr --port 3333 --open",
    "dev:spa": "vite --port 3333 --open",
    "clean": "rm -rf dist && rm -rf serverless/public && rm -rf serverless/api/renderer",
    "build": "pnpm clean && cross-env NODE_ENV=production vite-ssr build && ./scripts/move-dist.sh",
    "preview": "cd serverless && vercel dev",
    "deploy": "cd serverless && vercel",
    "deploy:prod": "cd serverless && vercel --prod"
  },
  "dependencies": {
    "@vueuse/core": "^12.5.0",
    "@vueuse/head": "^2.0.0",
    "nprogress": "^0.2.0",
    "prism-theme-vars": "^0.2.5",
    "vite-ssr": "^0.17.2",
    "vue": "^3.5.18",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@iconify/json": "^2.2.230",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/compiler-sfc": "^3.5.18",
    "@vue/server-renderer": "^3.5.18",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "markdown-it-prism": "^3.0.0",
    "node-fetch": "^3.3.2",
    "pnpm": "^10.14.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "unocss": "^66.3.3",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^5.0.0",
    "vite-plugin-md": "^0.21.5",
    "vite-plugin-pages": "^0.33.1",
    "vite-plugin-pwa": "^1.0.2",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-windicss": "^1.9.3"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config",
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}